<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <ContainerBaseImage>docker.io/lancemccarthy/skia-aspnet:10.0</ContainerBaseImage>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Aspire.Hosting.Seq" Version="13.0.0" />
    <PackageReference Include="Aspire.Seq" Version="13.0.0" />
    <PackageReference Include="Telerik.Drawing.Skia" Version="19.3.25.1111" />
    <PackageReference Include="Telerik.Reporting.OpenXmlRendering" Version="19.3.25.1111" />
    <PackageReference Include="Telerik.Reporting.Services.AspNetCore" Version="19.3.25.1111" />
    <PackageReference Include="Telerik.WebReportDesigner.Services" Version="19.3.25.1111" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\DevCraftAspire.ServiceDefaults\DevCraftAspire.ServiceDefaults.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Reports\BarcodesReport.trdp">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>
